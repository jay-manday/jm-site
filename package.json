{
  "name": "jm-site",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@vx/responsive": "^0.0.184",
    "aws-serverless-express": "^3.3.5",
    "babel-preset-react-app": "^7.0.1",
    "color": "^3.1.0",
    "cookie": "^0.3.1",
    "express": "^4.16.3",
    "grid-styled": "^5.0.2",
    "helmet": "^3.12.1",
    "in-viewport": "^3.6.0",
    "lodash": "^4.17.11",
    "next": "^8.0.3",
    "next-routes": "^1.4.2",
    "node-sass": "^4.9.2",
    "prismic-javascript": "^2.0.0-beta.0",
    "prismic-reactjs": "^0.3.1",
    "react": "^16.6.3",
    "react-delayed": "^0.2.3",
    "react-dom": "^16.6.3",
    "react-ga": "^2.5.7",
    "react-p5-wrapper": "^0.0.4",
    "react-parallax": "^2.1.0",
    "react-router-dom": "^4.3.1",
    "react-text-loop": "^2.0.1",
    "rebass": "^3.0.1",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-domain-manager": "^2.6.13",
    "serverless-http": "^1.9.1",
    "serverless-offline": "^4.4.2",
    "serverless-plugin-warmup": "^4.3.3-rc.1",
    "styled-components": "^4.1.3",
    "vx": "^0.0.1"
  },
  "devDependencies": {
    "eslint-plugin-react": "^7.1.0"
  },
  "scripts": {
    "dev": "node server/index.js",
    "postinstall": "npm run build",
    "start": "NODE_ENV=production node server/index.js",
    "build": "next build",
    "dev:server": "npm run build && node server.js",
    "prod:server": "npm run build && NODE_ENV=production node server.js",
    "package": "npm run build && serverless package --verbose",
    "deploy": "npm run package && serverless deploy"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
